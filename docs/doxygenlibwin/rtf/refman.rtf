450 {\cf18 void} {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAGZ" }{}}{\fldrslt {\cs37\ul\cf2 KMPSearch}}}
({\cf17 const} {\cf18 char} *pattern, {\cf17 const} {\cf18 char} *text) \{\par
451   {\cf18 int} m = strlen(pattern);\par
452   {\cf18 int} n = strlen(text);\par
453   {\cf18 int} *lps = ({\cf18 int} *)malloc(m * {\cf17 sizeof}({\cf18 int})); {\cf20 // Allocate memory for LPS array}\par
454   {\cf18 int} j = 0;\par
455   {\cf20 // Compute LPS array for the pattern}\par
456   lps[0] = 0;\par
457 \par
458   {\cf19 for} ({\cf18 int} i = 1; i < m;) \{\par
459     {\cf19 if} (pattern[i] == pattern[j]) \{\par
460       lps[i++] = ++j;\par
461     \} {\cf19 else} {\cf19 if} (j) \{\par
462       j = lps[j - 1];\par
463     \} {\cf19 else} \{\par
464       lps[i++] = 0;\par
465     \}\par
466   \}\par
467 \par
468   {\cf20 // Search for the pattern in the text}\par
469   {\cf19 for} ({\cf18 int} i = 0; i < n;) \{\par
470     {\cf19 if} (pattern[j] == text[i]) \{\par
471       ++j;\par
472       ++i;\par
473     \}\par
474 \par
475     {\cf19 if} (j == m) \{\par
476       printf({\cf22 "Pattern found at index %d\\n"}, i - j);\par
477       j = lps[j - 1];\par
478     \} {\cf19 else} {\cf19 if} (i < n && pattern[j] != text[i]) \{\par
479       j = j ? lps[j - 1] : 0;\par
480     \}\par
481   \}\par
